{
  "name": "renato-portfolio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev:sanity": "pnpm --filter=cms dev",
    "dev:app": "pnpm --filter=app dev",
    "dev": "concurrently --names \"CMS,APP\" -c \"bgBlue.bold,bgGreen.bold\" \"pnpm dev:sanity\" \"pnpm dev:app\"",
    "build": "pnpm turbo run build --filter=app",
    "a": "pnpm --filter=app analyze"
  },
  "dependencies": {
    "@sanity/client": "^7.6.0",
    "vercel": "^46.1.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "turbo": "^2.3.0"
  }
}